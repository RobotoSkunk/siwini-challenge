<%- layout('default') %>


<div style="margin: 0 auto; max-width: 500px; padding: 15px;">
	<h1 style="text-align: center;">DESCARGA DE FORMULARIOS</h1>

	<p style="color: var(--acc-red); font-size: 0.8em;">* Requerido.</p>

	<form method="POST">
		<div style="margin-top: 25px">
			<p>
				<label for="token">Ingrese el token de acceso para descargar los formularios</label>
				<br>
				<input type="text" name="token" id="token" required>
			</p>
	
			<button>Descargar</button>
		</div>
	<registro/formulario>
</div>
